    --opportunities-block-details:#333333;
    --opportunities-avatar-bg:#16a5e0;
    --opportunities-avatar-border-color:transparent;
    --opportunities-avatar-shadow:rgba(0,0,0,0.50);
    --opportunities-avatar-color:#ffffff;
    --opportunities-notification-bg:#16a5e0;
    --opportunities-notification-color:#ffffff;
    --opportunities-icons-bg:#ffffff;
    --opportunities-icons-border-color:#cccccc;
    --opportunities-icons-shadow:transparent;
    --opportunities-icons-color:#333333;
}
