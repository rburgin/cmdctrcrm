    --pipeline-header-label:#333;
    --pipeline-header-details:#333333;
    --pipeline-leads-bg:#16a5e0;
    --pipeline-leads-border:#16a5e0;
    --pipeline-leads-color:#ffffff;
    --opportunities-collapse-btn-bg:#ffffff;
    --opportunities-collapse-btn-color:#333333;
    --opportunities-collapse-btn-shadow:rgba(0,0,0,0.50);
    --opportunities-block-bg:#ffffff;
    --opportunities-block-border:#cccccc;
    --opportunities-block-shadow:rgb(0 0 0 / 0%);
